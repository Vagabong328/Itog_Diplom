        <Path Data="M1081.1667,229 L1087.8417,235.67499" HorizontalAlignment="Left" Height="6.008" Margin="1086.992,202.834,0,0" Stretch="Fill" Stroke="White" VerticalAlignment="Top" Width="6.008"/>
