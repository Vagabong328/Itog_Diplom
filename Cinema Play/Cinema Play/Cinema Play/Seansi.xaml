                    <Setter Property="Foreground" Value="Black"/>
                </Style>
            </DataGrid.ColumnHeaderStyle>
        </DataGrid>
        <Path Data="M537,159 L740.39482,159" Fill="Black" HorizontalAlignment="Left" Height="1" Margin="204,496.583,0,0" Stretch="Fill" Stroke="#FFFFB900" VerticalAlignment="Top" Width="885"/>
