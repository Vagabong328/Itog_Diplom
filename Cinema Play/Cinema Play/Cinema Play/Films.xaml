                </Style>
            </DataGrid.ColumnHeaderStyle>
        </DataGrid>
        <Path Data="M537,159 L740.39482,159" Fill="Black" HorizontalAlignment="Left" Height="1" Margin="166.5,462.583,0,0" Stretch="Fill" Stroke="#FFFFB900" VerticalAlignment="Top" Width="960"/>
        <Ellipse HorizontalAlignment="Left" Height="11.334" Margin="1077.667,200.667,0,0" Stroke="White" VerticalAlignment="Top" Width="11.333"/>
        <Button Content="Search" HorizontalAlignment="Left" Height="14" Margin="1078,201,0,0" VerticalAlignment="Top" Width="15" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="{x:Null}" Click="Search_Click" Loaded="Films_loaded"/>
        <Path Data="M1081.1667,229 L1087.8417,235.67499" HorizontalAlignment="Left" Height="6.008" Margin="1086.992,209.167,0,0" Stretch="Fill" Stroke="White" VerticalAlignment="Top" Width="6.008"/>
        <TextBox x:Name="Searchh" HorizontalAlignment="Left" Height="22" Margin="394,190,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="312" TextChanged="Searchh_TextChanged"/>
        <Label Content="Какой фильм Вас интересует?" HorizontalAlignment="Left" Height="33" Margin="166,184,0,0" VerticalAlignment="Top" Width="284" Foreground="White" FontSize="14"/>

    </Grid>
