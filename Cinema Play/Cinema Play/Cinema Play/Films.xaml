        <TextBox x:Name="Company" HorizontalAlignment="Left" Height="22" Margin="895,229,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168"/>
        <TextBox x:Name="Category" HorizontalAlignment="Left" Height="22" Margin="895,294,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168"/>
        <TextBox x:Name="Time" HorizontalAlignment="Left" Height="22" Margin="895,329,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168"/>
        <DatePicker x:Name="Year" HorizontalAlignment="Left" Height="26" Margin="895,261,0,0" VerticalAlignment="Top" Width="168" BorderBrush="{x:Null}"/>
        <Label Content="ФИЛЬМЫ" HorizontalAlignment="Left" Height="46" Margin="554,117,0,0" VerticalAlignment="Top" Width="170" Foreground="#FFFFB900" FontSize="36" FontFamily="Gilroy Bold"/>
