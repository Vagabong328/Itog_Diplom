            </Path.RenderTransform>
        </Path>
        <Image Margin="587.298,16,587.5,628.992" Source="белый лого.png" Stretch="Fill" RenderTransformOrigin="0.192,0.904"/>
        <Button Content="Назад" HorizontalAlignment="Left" Height="19" Margin="10,10,0,0" VerticalAlignment="Top" Width="37.5" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Click="Back_Click"/>
        <Button x:Name="Add" Content="Добавить" HorizontalAlignment="Left" Height="24" Margin="742,411,0,0" VerticalAlignment="Top" Width="90" Background="#FFFFB900" FontFamily="Gilroy Bold" FontSize="14" Click="Add_Click"/>
        <Button x:Name="Del" Content="Удалить" HorizontalAlignment="Left" Height="24" Margin="872,411,0,0" VerticalAlignment="Top" Width="90" Background="#FFFFB900" FontFamily="Gilroy Bold" FontSize="14" Click="Del_Click"/>
        <Button x:Name="Upp" Content="Изменить" HorizontalAlignment="Left" Height="24" Margin="999,410.583,0,0" VerticalAlignment="Top" Width="90" Background="#FFFFB900" FontFamily="Gilroy Bold" FontSize="14" Click="Upp_Click"/>
        <Label Content="Название фильма" HorizontalAlignment="Left" Height="33" Margin="742,191,0,0" VerticalAlignment="Top" Width="147" Foreground="White" FontSize="14"/>
        <Label Content="Компания" HorizontalAlignment="Left" Height="33" Margin="742,224,0,0" VerticalAlignment="Top" Width="108" Foreground="White" FontSize="14"/>
        <Label Content="Год выпуска" HorizontalAlignment="Left" Height="34" Margin="742,258,0,0" VerticalAlignment="Top" Width="108" Foreground="White" FontSize="14"/>
        <Label Content="Жанр" HorizontalAlignment="Left" Height="32" Margin="742,292,0,0" VerticalAlignment="Top" Width="108" Foreground="White" FontSize="14"/>
        <Label Content="Длительность" HorizontalAlignment="Left" Height="34" Margin="742,324,0,0" VerticalAlignment="Top" Width="108" Foreground="White" FontSize="14"/>
